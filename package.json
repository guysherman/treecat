{
  "name": "sql-commander",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc --project ./tsconfig.json",
    "typecheck": "tsc --project ./tsconfig.json --noemit",
    "test": "scripts/test-ci.sh",
    "test:treecat": "jest --testPathPattern=src/treecat/ --testPathIgnorePatterns=dist/ --runInBand",
    "lint": "eslint src/**/*.ts",
    "run-dev": "ts-node src/index.tsx",
    "inspect-dev": "node -r ts-node/register --inspect-brk src/index.tsx",
    "test-watch:treecat": "jest --testPathPattern=src/treecat/ --testPathIgnorePatterns=dist/ --watch --runInBand --no-cache"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/knex": "^0.16.1",
    "@types/node": "^16.3.1",
    "@types/pg": "^8.6.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^7.28.0",
    "eslint-config-standard": "latest",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^27.0.4",
    "knex": "^0.95.6",
    "pinst": "^2.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@types/blessed": "^0.1.17",
    "@types/minimist": "^1.2.1",
    "@types/minimist-options": "^4.0.1",
    "@yarnpkg/pnpify": "^3.0.0-rc.6",
    "blessed": "^0.1.81",
    "minimist": "^1.2.5",
    "minimist-options": "^4.1.0",
    "pg": "^8.6.0"
  }
}
